{
  "name": "@desci-labs/desci-sync",
  "version": "0.0.2",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "NODE_PATH=./dist node ./dist/index.js",
    "dev": "wrangler dev --port 5445 --ip 0.0.0.0 --local-protocol http",
    "docker:dev": "docker compose --file ../docker-compose.sync.yml --compatibility up --build",
    "watch-compile": "debug=* tsc -w --preserveWatchOutput",
    "test": "NODE_ENV=test mocha --colors --timeout 20000 --exit",
    "lint": "eslint --max-warnings 0 --ext .js,ts src",
    "lint-staged-husky": "lint-staged",
    "lint-prettier": "prettier --config .prettierrc --list-different '{src,test}/**/*.{ts,js,css,scss}'",
    "prettier": "prettier --config .prettierrc --write './**/*.{ts,js,css,scss,json,md}'",
    "commit": "git-cz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*": [
      "pretty-quick --staged"
    ],
    "{src,test}/**/*.{js,ts}": [
      "eslint --max-warnings 0"
    ]
  },
  "resolutions": {
    "cliui": "^8.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "chai": "4.3.4",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-no-array-reduce": "^1.0.62",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "11.1.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@automerge/automerge-repo": "^2.0.0-alpha.14",
    "@automerge/automerge-repo-network-websocket": "^2.0.0-alpha.14",
    "@automerge/automerge-repo-react-hooks": "^1.0.19",
    "@automerge/automerge-repo-storage-indexeddb": "^2.0.0-alpha.14",
    "@cloudflare/workers-types": "^4.20241022.0",
    "isomorphic-ws": "^5.0.0",
    "partykit": "^0.0.111",
    "partyserver": "^0.0.57",
    "partysocket": "^1.0.2",
    "pg": "^8.13.1",
    "wrangler": "3.80.0",
    "ws": "^8.14.2"
  }
}
