{
  "name": "@desci-labs/desci-server",
  "version": "0.3.4",
  "description": "Manage DeSci Nodes state: users, nodes orchestration, decentralized storage, blockchain resolvers",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "dev:tsx": "node --import=tsx --watch ./src/index.ts",
    "watch-compile": "debug=* tsc -w --preserveWatchOutput",
    "gen-docs": "tsx ./src/swagger.ts",
    "watch-dev": "debug=* nodemon --watch dist -e js ./dist/index.js",
    "script:fix-mirrors": "tsx ./src/scripts/retry-mirrors.ts",
    "script:upgrade-manifests": "tsx ./src/scripts/upgradeManifests.ts",
    "script:fill-research-fields": "tsx ./src/scripts/fill-research-fields.ts",
    "script:fix-data-refs": "tsx --inspect=0.0.0.0:9277 ./src/scripts/dataRefDoctor.ts",
    "script:active-users": "tsx ./src/scripts/activeUsers.ts",
    "script:invalidate-redis-cache": "tsx ./src/scripts/invalidate-redis-cache.ts",
    "script:fix-publish": "tsx --inspect=0.0.0.0:9277 ./src/scripts/fixPublish.ts",
    "script:increase-base-drive-storage": "tsx ./src/scripts/increase-base-drive-storage.ts",
    "script:testing": "tsx --inspect=0.0.0.0:9277 ./src/scripts/test-exec.ts",
    "script:backfill-legacy-dpids": "tsx --inspect=0.0.0.0:9277 ./src/scripts/fix-global-legacy-dpids.ts",
    "script:es-index-published-nodes": "tsx --inspect=0.0.0.0:9277 ./src/scripts/index-published-nodes-elastic-search.ts",
    "script:preload-ai-api": "tsx --inspect=0.0.0.0:9277 ./src/scripts/preload-ai-api.ts",
    "script:migrate-draft-trees": "tsx ./src/scripts/migrate-draft-trees.ts",
    "script:endorse-dpid": "tsx ./src/scripts/endorse-dpid.ts",
    "script:fix-dpid": "tsx ./src/scripts/fix-dpid.ts",
    "script:seed-social-data": "tsx ./src/scripts/seed-social-data.ts",
    "script:DESTRUCTIVE-clear-social-data": "tsx ./src/scripts/DESTRUCTIVE-clear-social-data.ts",
    "script:seed-community-member": "tsx ./src/scripts/seed-community-members.ts",
    "script:backfill-annotations": "tsx ./src/scripts/backfill-annotations.ts",
    "script:prune-auth-tokens": "tsx ./src/scripts/prune-auth-tokens.ts",
    "script:backfill-radar": "tsx ./src/scripts/backfill-radar.ts",
    "script:pruneExternalPublications": "tsx ./src/scripts/pruneExternalPublications.ts",
    "build": "rimraf dist && tsc && yarn copy-files; if [ \"$SENTRY_AUTH_TOKEN\" ]; then yarn sentry:sourcemaps; else echo 'SENTRY_AUTH_TOKEN not set, sourcemaps will not upload'; fi",
    "copy-files": "copyfiles -u 1 src/**/*.cjs dist/",
    "generate": "npx prisma generate",
    "migrate:local": "DATABASE_URL=postgresql://walter:white@localhost:5433/boilerplate npx prisma migrate dev",
    "migrate": "yarn generate && npx prisma migrate deploy --preview-feature",
    "start": "NODE_PATH=./dist node ./dist/index.js",
    "prepare": "",
    "lint": "eslint --max-warnings 0 --ext .js,ts src",
    "lint-staged-husky": "lint-staged",
    "lint-prettier": "prettier --config .prettierrc --list-different '{src,test}/**/*.{ts,js,css,scss}'",
    "prettier": "prettier --config .prettierrc --write './**/*.{ts,js,css,scss,json,md}'",
    "test": "vitest --run --config vitest.config.ts",
    "test:debug": "vitest --run --config vitest.config.ts --inspect-brk",
    "db:forward": "kubectl run --env REMOTE_HOST=$REMOTE_HOST --env REMOTE_PORT=5432 --env LOCAL_PORT=8080 --port 8080 --image marcnuri/port-forward test-port-forward ;  kubectl port-forward test-port-forward 8080:8080",
    "docker:dev": "../dockerDev.sh",
    "docker:test": "export SKIP_NODES_LIB=1 && cd .. && ./integrationTestRunner.sh",
    "docker:stage": "docker compose --file docker-compose.yml --file docker-compose.stage.yml --compatibility up --build",
    "docker:prod": "../dockerProd.sh",
    "podman:dev": "podman-compose --file docker-compose.yml --file docker-compose.dev.yml up --build",
    "email-dev": "email dev --dir ./src/templates/emails --port 3777",
    "check-deps": "npx npm-check",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org desci-labs --project nodes-backend ./dist && sentry-cli sourcemaps upload --org desci-labs --project nodes-backend ./dist && echo 'Sentry sourcemaps uploaded'",
    "madge": "tsc && npx --yes madge --circular --image circularDepGraph.svg dist/index.js && open circularDepGraph.svg 2>/dev/null || xdg-open circularDepGraph.svg"
  },
  "dependencies": {
    "@amplitude/analytics-node": "^1.5.10",
    "@automerge/automerge-repo": "^1.0.19",
    "@automerge/automerge-repo-network-websocket": "^1.0.19",
    "@awesome-myst/myst-zod": "^0.6.1",
    "@aws-sdk/client-s3": "^3.537.0",
    "@aws-sdk/client-sqs": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@desci-labs/desci-codex-lib": "^2.0.0-rc5",
    "@desci-labs/desci-contracts": "^0.2.7",
    "@desci-labs/desci-models": "^0.2.28",
    "@elastic/elasticsearch": "^8.14.0",
    "@honeycombio/opentelemetry-node": "^0.3.2",
    "@ipld/dag-pb": "^4.0.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.37",
    "@penseapp/discord-notification": "^2.0.9",
    "@prisma/client": "4.10.1",
    "@react-email/components": "0.0.15",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "8.29.0",
    "@sentry/profiling-node": "8.32.0",
    "@sentry/tracing": "^7.12.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "arweave": "^1.10.18",
    "aws-sdk": "^2.1186.0",
    "axios": "^1.3.5",
    "axios-retry": "^4.5.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.5",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "ethers": "^5.6.9",
    "express": "^4.17.1",
    "fast-fuzzy": "^1.12.0",
    "form-data": "^4.0.0",
    "gaxios": "^6.3.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^133.0.0",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "3.0.0-beta.0",
    "ipfs-unixfs": "^11.1.3",
    "it-to-buffer": "^4.0.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "kill-port": "^2.0.1",
    "kubo-rpc-client": "^3.0.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mathjs": "^14.5.2",
    "mixpanel": "^0.18.1",
    "mkdirp": "^1.0.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "multiformats": "^13.1.0",
    "neverthrow": "^8.2.0",
    "node-fetch": "^3.3.2",
    "openid-client": "^6.6.4",
    "pg": "^8.12.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "pino-std-serializers": "^7.0.0",
    "prisma": "4.10.1",
    "qs": "^6.12.0",
    "react-email": "2.1.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "remixml": "^7.0.5",
    "request": "^2.88.2",
    "rimraf": "^5.0.1",
    "short-unique-id": "^4.4.4",
    "siwe": "^1.1.6",
    "socket.io": "^4.8.0",
    "stripe": "^18.5.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "url-safe-base64": "1.2.0",
    "uuid": "^8.3.2",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.2",
    "yaml": "^2.7.0",
    "yauzl": "^2.10.0",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.3"
  },
  "devDependencies": {
    "@sentry/cli": "^2.55.0",
    "@types/archiver": "^6.0.2",
    "@types/body-parser": "^1.19.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash-es": "^4.17.12",
